<Window x:Class="WPFVisifireChartsApp.Window1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Window1" Height="456" Width="676" Background="#FFD9CDCD"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d"
    xmlns:vc="clr-namespace:Visifire.Charts;assembly=WPFVisifire.Charts" 
        WindowState="Normal">
    
    <Grid x:Name="LayoutRoot" Background="White" >
        <vc:Chart Width="400" Height="300" Theme="Theme1">
            <vc:Chart.Series>
                <vc:DataSeries RenderAs="Column">
                    <vc:DataSeries.DataPoints>
                        <vc:DataPoint YValue="35" />
                        <vc:DataPoint YValue="22" />
                        <vc:DataPoint YValue="54" />
                        <vc:DataPoint YValue="23" />
                        <vc:DataPoint YValue="33" />
                        <vc:DataPoint YValue="43" />
                    </vc:DataSeries.DataPoints>
                </vc:DataSeries>
            </vc:Chart.Series>
        </vc:Chart>


    </Grid>
</Window>
