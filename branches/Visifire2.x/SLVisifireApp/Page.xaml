<UserControl x:Class="SLVisifireApp.Page"
xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
xmlns:vc="clr-namespace:Visifire.Charts;assembly=SLVisifire.Charts"
Width="Auto" Height="Auto"
xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d"
>
    <Grid x:Name="LayoutRoot" Width="500" Height="300">
        <vc:Chart Theme="Theme1">
        
            <vc:Chart.Series>
                <vc:DataSeries RenderAs="Column">
                    <vc:DataSeries.DataPoints>
                        <vc:DataPoint XValue="1/1/2008" YValue="10"/>
                        <vc:DataPoint XValue="1/2/2008" YValue="20"/>
                        <vc:DataPoint XValue="1/3/2008" YValue="20"/>
                        <vc:DataPoint XValue="1/4/2008" YValue="20"/>
                        <vc:DataPoint XValue="1/5/2008" YValue="10"/>
                    </vc:DataSeries.DataPoints>
                </vc:DataSeries>
            </vc:Chart.Series>
        </vc:Chart>
    </Grid>
</UserControl>

    
