<UserControl xmlns:basics="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls" x:Class="SLVisifireApp.Page"
xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
xmlns:vc="clr-namespace:Visifire.Charts;assembly=SLVisifire.Charts"
Width="Auto" Height="Auto"
>
    <Grid x:Name="LayoutRoot">

        <vc:Chart Width="500" Height="300" Theme="Theme2" Name="MyChart">

            <vc:Chart.Titles>
                <vc:Title Text="Visifire Chart"/>
            </vc:Chart.Titles>

            <vc:Chart.Series>
                <vc:DataSeries RenderAs="CandleStick">
                    <vc:DataSeries.DataPoints>
                        <vc:DataPoint XValue="1" YValues="10,20,30,5"/>
                        <vc:DataPoint XValue="2" YValues="20,22,28,18"/>
                        <vc:DataPoint XValue="3" YValues="20,10,22,8"/>
                        <vc:DataPoint XValue="4" YValues="20,30,32,8"/>
                        <vc:DataPoint XValue="5" YValues="20,30,32,8"/>
                    </vc:DataSeries.DataPoints>
                </vc:DataSeries>

            </vc:Chart.Series>
        </vc:Chart>
    </Grid>
</UserControl>