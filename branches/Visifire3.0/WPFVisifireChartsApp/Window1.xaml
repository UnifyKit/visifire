<Window x:Class="WPFVisifireChartsApp.Window1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Window1"
    xmlns:vc="clr-namespace:Visifire.Charts;assembly=WPFVisifire.Charts"
    WindowState="Normal">
    
    <Grid x:Name="LayoutRoot" Width="500" Height="300">
        
        <vc:Chart Width="500" Height="300"
                  Theme="Theme2" Name="MyChart">

            <vc:Chart.Series>
                <vc:DataSeries RenderAs="Line" >
                    <vc:DataSeries.DataPoints>
                        <vc:DataPoint XValue="1000" YValue="10"/>
                        <vc:DataPoint XValue="2000" YValue="20"/>
                        <vc:DataPoint AxisXLabel="Hi" XValue="3000" Enabled="False" YValue=""/>
                        <vc:DataPoint XValue="4000" YValue="20"/>
                        <vc:DataPoint XValue="5000" YValue="20"/>
                    </vc:DataSeries.DataPoints>
                </vc:DataSeries>
            </vc:Chart.Series>
        </vc:Chart>
    </Grid>
    
</Window>
