<Window x:Class="WPFVisifireChartsApp.Window1"
xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
xmlns:vc="clr-namespace:Visifire.Charts;assembly=WPFVisifire.Charts" Height="362" Width="683">

    <Grid x:Name="LayoutRoot">
        <vc:Chart x:Name="My_Chart" xmlns:vc="clr-namespace:Visifire.Charts;assembly=WPFVisifire.Charts"
                  Width="500" Height="300" Theme="Theme1">

            <vc:Chart.Titles>
                <vc:Title Text="Visifire Chart"/>
            </vc:Chart.Titles>

            <vc:Chart.Series>
                <vc:DataSeries RenderAs="Radar">
                    <vc:DataSeries.DataPoints>
                        <vc:DataPoint YValue="18"/>
                        <vc:DataPoint YValue="28"/>
                        <vc:DataPoint YValue="38"/>
                        <vc:DataPoint YValue="48"/>
                        <vc:DataPoint YValue="58"/>
                    </vc:DataSeries.DataPoints>
                </vc:DataSeries>
            </vc:Chart.Series>
        </vc:Chart>
    </Grid>
</Window>