<UserControl xmlns:basics="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls" x:Class="SLVisifireApp.Page"
xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
xmlns:vc="clr-namespace:Visifire.Charts;assembly=SLVisifire.Charts"
Width="Auto" Height="Auto"
>   
    <Grid x:Name="LayoutRoot" Width="500" Height="400">
        <vc:Chart Theme="Theme1" Width="500" Height="300">
            
            <vc:Chart.Series>
                <vc:DataSeries RenderAs="Column">
                    <vc:DataSeries.DataPoints>
                        <vc:DataPoint YValue="12" />
                        <vc:DataPoint YValue="24"/>
                        <vc:DataPoint YValue="32"/>
                        <vc:DataPoint YValue="18" />
                        <vc:DataPoint YValue="28" />
                    </vc:DataSeries.DataPoints>
                </vc:DataSeries>
            </vc:Chart.Series>
        </vc:Chart>
    </Grid>
</UserControl>
