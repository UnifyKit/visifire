<UserControl xmlns:basics="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls" x:Class="SLVisifireApp.Page"
xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
xmlns:vc="clr-namespace:Visifire.Charts;assembly=SLVisifire.Charts"
Width="Auto" Height="Auto"
>   
    <Grid x:Name="LayoutRoot">
        <vc:Chart Width="500" Height="300" Theme="Theme1" x:Name="MyChart">

            <vc:Chart.Series>
                <vc:DataSeries RenderAs="Column" LabelEnabled="True">
                    <vc:DataSeries.DataPoints>
                        <vc:DataPoint YValue="32"/>
                        <vc:DataPoint YValue="5"/>
                        <vc:DataPoint YValue="2"/>
                        <vc:DataPoint YValue="75"/>
                        <vc:DataPoint YValue="75"/>
                    </vc:DataSeries.DataPoints>
                </vc:DataSeries>
            </vc:Chart.Series>
        </vc:Chart>
    </Grid>
</UserControl>
