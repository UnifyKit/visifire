<vc:Chart xmlns:vc="clr-namespace:Visifire.Charts;assembly=Visifire.Charts" 
          Theme="Theme2" AnimationType="Type4" ColorSet="Picasso" Width="350" Height="400" View3D="True">

  <vc:Title Text="Age-Wise Distribution of Google and Yahoo Users" />
  <vc:AxisX Title="Age" />
  <vc:AxisY Title="Percentage Distribution" ValueFormatString="#0.##'%'" />
  
   <vc:DataSeries Name="Google" RenderAs="Bar">
    <vc:DataPoint AxisLabel="18-24" YValue="19.33" />
    <vc:DataPoint AxisLabel="25-34" YValue="23.13" />
    <vc:DataPoint AxisLabel="35-44" YValue="22.34" />
    <vc:DataPoint AxisLabel="45-54" YValue="17.97" />
    <vc:DataPoint AxisLabel="55+" YValue="17.23" />
  </vc:DataSeries>
  
   <vc:DataSeries Name="Yahoo" RenderAs="Bar">
    <vc:DataPoint YValue="17.91" />
    <vc:DataPoint YValue="20.37" />
    <vc:DataPoint YValue="22.93" />
    <vc:DataPoint YValue="20.07" />
    <vc:DataPoint YValue="18.72" />
  </vc:DataSeries>

</vc:Chart>
