<vc:Chart xmlns:vc="clr-namespace:Visifire.Charts;assembly=SLVisifire.Charts"
          Theme="Theme3" AnimationEnabled="False" ColorSet="Visifire2" View3D="False">

  <vc:Chart.Titles>
    <vc:Title Text="Visifire Bubble Chart" />
  </vc:Chart.Titles>

